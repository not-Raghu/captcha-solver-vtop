{
  "manifest_version": 3,
  "name": "vtop hate",
  "version": "1.0.0",
  "author": "raghu",
  "description": "solves those annoying captchas on vtop portal for you",
  "host_permissions": ["https://vtop.vitap.ac.in/vtop/login"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.png",
    "default_title": "vtop hate"
  },
  "content_scripts": [
    {
      "matches": ["https://vtop.vitap.ac.in/vtop/login*"],
      "js": ["main.js"]
    }
  ],
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  }
}
